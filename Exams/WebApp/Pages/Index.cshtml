@page
@model IndexModel
@{
    ViewData["Title"] = "Furnitures page";
}

<table>
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Quantity</th>
    </tr>
    @foreach (var item in Model.Items)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Quantity</td>
        </tr>
    }
</table>
